{
  "name": "js-set-extension",
  "version": "1.0.0",
  "description": "Extending the Set class in order to support mathematical set properties and operations.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist/** && babel lib --out-dir dist --ignore *.tests.js,testHelpers/*",
    "lint": "standard",
    "lint-test": "npm run lint && npm test",
    "prepublishOnly": "npm run lintAndTest && npm run copyFiles && npm run build",
    "test": "mocha --compilers js:@babel/register --recursive \"lib/**/*.tests.js\"",
    "test-watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "standard": "*"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "targets": "> 0.25%, not dead",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jankapunkt/js-set-extension.git"
  },
  "keywords": [
    "set",
    "math",
    "superset",
    "subset"
  ],
  "author": "Jan KÃ¼ster <jkuester@uni-bremen.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jankapunkt/js-set-extension/issues"
  },
  "homepage": "https://github.com/jankapunkt/js-set-extension#readme"
}
